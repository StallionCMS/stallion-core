<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>{{ meta.title }}</title>

    <!-- Bootstrap core CSS -->
    <link href="{{ bootstrap.min.css }}" rel="stylesheet">




    {% block builtin_head_bundles %}
    {{ assets.definedBundle("alwaysHeadStylesheets") }}
    {{ assets.definedBundle("adminHeadStylesheets") }}
    {% endblock %}
    {% block bundle_css %}
    {{ assets.bundle("site.bundle.css") }}
    {% endblock %}
    {% block extra_head %}
    {% endblock %}

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>
    {% block body_content %}
    {% endblock %}
    {% block builtin_footer_bundles %}
    {{ assets.definedBundle("alwaysFooterJavascripts") }}
    {{ assets.definedBundle("adminFooterJavascripts") }}
    {% endblock %}
    {% block pre_end_body %}
    {% endblock %}
  </body>
</html>



